{
  "ver" : "1.0.0",
  "authors" : ["tgu2"],
  "contact" : "help@kbase.us",
  "visible" : true,
  "categories" : ["active","expression","assembly"],
  "widgets" : {
    "input" : null,
    "output" : "no-display"
  },
   "parameters" : [ {
    "id" : "alignment_object_ref",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : ["KBaseRNASeq.RNASeqAlignmentSet" , "KBaseRNASeq.RNASeqAlignment", "KBaseSets.ReadsAlignmentSet"]
    }
  },{
    "id" : "expression_suffix",
    "optional" : false,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "_expression" ],
    "field_type" : "text"
  },{
    "id" : "expression_set_suffix",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "_expression_set" ],
    "field_type" : "text"
  },{
   "id" : "min_isoform_abundance",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "0.1" ],
    "field_type" : "text",
    "text_options" : {
      "validate_as": "float"
    }
  }, {
   "id" : "min_length",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "200" ],    
    "field_type" : "text",
    "text_options" : {
      "validate_as": "int"
    }
  }, {
    "id" : "junction_base",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "10" ],    
    "field_type" : "text",
    "text_options" : {
      "validate_as": "int"
    }
  }, {
    "id" : "junction_coverage",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "1" ],     
    "field_type" : "text",
    "text_options" : {
      "validate_as": "float"
    }
  }, {
    "id" : "min_read_coverage",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "2.5" ],
    "field_type" : "text",
    "text_options" : {
      "validate_as": "float"
    }
  }, {
    "id" : "min_locus_gap_sep_value",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "50" ],
    "field_type" : "text",
    "text_options" : {
      "validate_as": "int"
    }
   },{
    "id" : "stringtie_genome_name",
    "optional" : true,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text"
   },{
    "id" : "transcript_label",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text"
  }],
  "parameter-groups": [
      {
	      "id": "novel_isoforms",
	      "parameters": ["stringtie_genome_name", "transcript_label"],
	      "optional": true,
	      "allow_multiple": false,
	      "with_border": true
      }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "kb_stringtie",
      "method" : "run_stringtie_app",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property" : "workspace_name"
        },
        {
          "input_parameter" : "alignment_object_ref",
          "target_type_transform": "resolved-ref",
          "target_property" : "alignment_object_ref"
        },
        {
            "input_parameter" : "expression_suffix",
            "target_property" : "expression_suffix"
        },
        {
            "input_parameter" : "expression_set_suffix",
            "target_property" : "expression_set_suffix"
        },
        {
          "input_parameter" : "novel_isoforms",
          "target_property" : "novel_isoforms"
        },
        {
          "constant_value"  : 1,
          "target_property" : "ballgown_mode"
        },        
        {
          "constant_value"  : 1,
          "target_property" : "skip_reads_with_no_ref"
        },       
        {
          "constant_value"  : 10,
          "target_property" : "num_threads"
        },      
        {
          "input_parameter" : "min_length",
          "target_property" : "min_length"
        },
        {
          "input_parameter" : "min_isoform_abundance",
          "target_property" : "min_isoform_abundance"
        },
        {
          "input_parameter" : "junction_base",
          "target_property" : "junction_base"
        },     
        {
          "input_parameter" : "junction_coverage",
          "target_property" : "junction_coverage"
        },    
        {
          "input_parameter" : "min_read_coverage",
          "target_property" : "min_read_coverage"
        },   
        {
          "input_parameter" : "min_locus_gap_sep_value",
          "target_property" : "min_locus_gap_sep_value"
        }],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
          "service_method_output_path": [0, "result_directory"],
          "target_property": "result_directory"
        },
        {
         "service_method_output_path": [0,"expression_obj_ref"],
         "target_property": "expression_obj_ref"
        },
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        },
        {
          "constant_value": "16",
          "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field" : "docker"
}
